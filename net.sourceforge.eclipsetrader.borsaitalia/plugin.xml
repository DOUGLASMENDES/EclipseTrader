<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="net.sourceforge.eclipsetrader.borsaitalia"
   name="Borsa Italiana"
   version="0.11.1"
   provider-name="Sourceforge.net"
   class="net.sourceforge.eclipsetrader.borsaitalia.BorsaitaliaPlugin">

   <runtime>
      <library name="borsaitalia.jar">
         <export name="*"/>
      </library>
   </runtime>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="net.sourceforge.eclipsetrader"/>
   </requires>
   <extension
         point="net.sourceforge.eclipsetrader.chartDataProvider">
      <provider
            label="Borsa Italiana"
            class="net.sourceforge.eclipsetrader.borsaitalia.ChartDataProvider"
            id="borsaitalia.chart"/>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="net.sourceforge.eclipsetrader.borsaitalia.GeneralPreferencePage"
            name="Borsa Italiana"
            id="borsaitalia.generalPreferences"/>
   </extension>
   <extension
         point="net.sourceforge.eclipsetrader.backfillDataProvider">
      <provider
            label="Borsa Italiana"
            class="net.sourceforge.eclipsetrader.borsaitalia.BackfillDataProvider"
            id="borsaitalia.backfill"/>
   </extension>
   <extension
         point="net.sourceforge.eclipsetrader.indexProvider">
      <provider
            class="net.sourceforge.eclipsetrader.borsaitalia.IndexDataProvider"
            name="Borsa Italiana"
            id="borsaitalia.indexProvider">
      <index
            ticker="SPMIB"
            label="S/P MIB"
            symbol="IS945"/>
      <index
            ticker="MIBTEL"
            label="MIBTEL"
            symbol="II999"/>
      <index
            ticker="ALL STARS"
            label="ALL STARS"
            symbol="II952"/>
      <index
            ticker="STAR"
            label="STAR"
            symbol="II951"/>
      <index
            ticker="techSTAR"
            label="techSTAR"
            symbol="II950"/>
      <index
            ticker="MIDEX"
            label="MIDEX"
            symbol="II932"/>
      <index
            ticker="MIBTEL-s"
            label="MIBTEL-s"
            symbol="II935"/>
      <index
            ticker="MIB30-s"
            label="MIB30-s"
            symbol="II933"/>
      </provider>
   </extension>

</plugin>
