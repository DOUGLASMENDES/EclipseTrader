<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="net.sourceforge.eclipsetrader.yahoo"
            label="%actionSet.label"
            visible="true">
         <action
               class="net.sourceforge.eclipsetrader.yahoo.actions.NewSecurityAction"
               icon="icons/us.gif"
               id="net.sourceforge.eclipsetrader.yahoo.newSecurity"
               label="%securityWizard.action.label"
               menubarPath="file/newWizard/plugins"
               style="push"/>
         <action
               class="net.sourceforge.eclipsetrader.yahoo.actions.NewGermanSecurityAction"
               icon="icons/de.gif"
               id="net.sourceforge.eclipsetrader.yahoo.newGermanSecurity"
               label="%german.securityWizard.action.label"
               menubarPath="file/newWizard/plugins"
               style="push"/>
         <action
               class="net.sourceforge.eclipsetrader.yahoo.actions.NewFrenchSecurityAction"
               icon="icons/fr.gif"
               id="net.sourceforge.eclipsetrader.yahoo.newFranceSecurity"
               label="%french.securityWizard.action.label"
               menubarPath="file/newWizard/plugins"
               style="push"/>
         <action
               class="net.sourceforge.eclipsetrader.yahoo.actions.NewIndexAction"
               icon="icons/world.png"
               id="net.sourceforge.eclipsetrader.yahoo.newIndex"
               label="%index.securityWizard.action.label"
               menubarPath="file/newWizard/top"
               style="push"/>
      </actionSet>
   </extension>
   <extension
         point="org.eclipse.ui.importWizards">
      <wizard
            class="net.sourceforge.eclipsetrader.yahoo.internal.updater.ui.SecuritiesListImportWizard"
            icon="icons/yahoo.gif"
            id="net.sourceforge.eclipsetrader.yahoo.import.securities"
            name="%securities.import.wizard.name">
         <description>
            %securities.import.wizard.description
         </description>
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="net.sourceforge.eclipsetrader.plugins"
            class="net.sourceforge.eclipsetrader.yahoo.preferences.PluginPreferencesPage"
            id="net.sourceforge.eclipsetrader.yahoo.quote"
            name="%preferencePage.name"/>
      <page
            category="net.sourceforge.eclipsetrader.news"
            class="net.sourceforge.eclipsetrader.yahoo.preferences.NewsPreferencesPage"
            id="net.sourceforge.eclipsetrader.yahoo.news"
            name="%news.preferencePage.name"/>
      <page
            category="net.sourceforge.eclipsetrader.news"
            class="net.sourceforge.eclipsetrader.yahoo.preferences.FrenchNewsPreferencesPage"
            id="net.sourceforge.eclipsetrader.yahoo.news.france"
            name="%france.news.preferencePage.name"/>
      <page
            category="net.sourceforge.eclipsetrader.news"
            class="net.sourceforge.eclipsetrader.yahoo.preferences.ItalianNewsPreferencesPage"
            id="net.sourceforge.eclipsetrader.yahoo.news.italy"
            name="%italy.news.preferencePage.name"/>
   </extension>
   <extension
         point="net.sourceforge.eclipsetrader.core.feeds">
      <feed
            id="%feed.id"
            name="%feed.name">
         <history class="net.sourceforge.eclipsetrader.yahoo.HistoryFeed"/>
         <quote class="net.sourceforge.eclipsetrader.yahoo.Feed">
         </quote>
      </feed>
   </extension>
   <extension
         point="net.sourceforge.eclipsetrader.core.news">
      <provider
            class="net.sourceforge.eclipsetrader.yahoo.NewsProvider"
            id="net.sourceforge.eclipsetrader.yahoo.news"
            name="%news.provider.name"/>
      <provider
            class="net.sourceforge.eclipsetrader.yahoo.ItalianNewsProvider"
            id="net.sourceforge.eclipsetrader.yahoo.news.italy"
            name="%italy.news.provider.name"/>
      <provider
            class="net.sourceforge.eclipsetrader.yahoo.FrenchNewsProvider"
            id="net.sourceforge.eclipsetrader.yahoo.news.france"
            name="%france.news.provider.name"/>
   </extension>

</plugin>
