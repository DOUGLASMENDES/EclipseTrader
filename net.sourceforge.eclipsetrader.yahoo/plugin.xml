<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="net.sourceforge.eclipsetrader.yahoo"
            label="Yahoo"
            visible="true">
         <action
               class="net.sourceforge.eclipsetrader.yahoo.actions.NewSecurityAction"
               icon="icons/us.gif"
               id="net.sourceforge.eclipsetrader.yahoo.newSecurity"
               label="U.S. Stock Security"
               menubarPath="file/newWizard/plugins"
               style="push"/>
         <action
               class="net.sourceforge.eclipsetrader.yahoo.actions.NewGermanSecurityAction"
               icon="icons/de.gif"
               id="net.sourceforge.eclipsetrader.yahoo.newGermanSecurity"
               label="German Stock Security"
               menubarPath="file/newWizard/plugins"
               style="push"/>
         <action
               class="net.sourceforge.eclipsetrader.yahoo.actions.NewFrenchSecurityAction"
               icon="icons/fr.gif"
               id="net.sourceforge.eclipsetrader.yahoo.newFranceSecurity"
               label="French Stock Security"
               menubarPath="file/newWizard/plugins"
               style="push"/>
      </actionSet>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="net.sourceforge.eclipsetrader.plugins"
            class="net.sourceforge.eclipsetrader.yahoo.preferences.NewsPreferencesPage"
            id="net.sourceforge.eclipsetrader.yahoo.news"
            name="Yahoo! Finance"/>
   </extension>
   <extension
         point="net.sourceforge.eclipsetrader.core.feeds">
      <feed
            id="net.sourceforge.eclipsetrader.yahoo"
            name="Yahoo! Finance">
         <history class="net.sourceforge.eclipsetrader.yahoo.HistoryFeed"/>
         <quote class="net.sourceforge.eclipsetrader.yahoo.Feed">
         </quote>
      </feed>
   </extension>
   <extension
         point="net.sourceforge.eclipsetrader.core.news">
      <provider
            class="net.sourceforge.eclipsetrader.yahoo.NewsProvider"
            id="net.sourceforge.eclipsetrader.yahoo.news"
            name="Yahoo! Finance U.S."/>
      <provider
            class="net.sourceforge.eclipsetrader.yahoo.ItalianNewsProvider"
            id="net.sourceforge.eclipsetrader.yahoo.news.italy"
            name="Yahoo! Finance Italy"/>
   </extension>

</plugin>
