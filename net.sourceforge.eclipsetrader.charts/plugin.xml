<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension-point id="indicators" name="%extension-point.name.indicators" schema="schema/indicators.exsd"/>
   <extension-point id="objects" name="%extension-point.name.objects" schema="schema/objects.exsd"/>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="net.sourceforge.eclipsetrader.charts"
            label="%actionSet.label.charts">
         <menu
               id="chartMenu"
               label="%menu.label.chart"
               path="charts">
            <separator name="begin"/>
            <separator name="group1"/>
            <separator name="group2"/>
            <separator name="group3"/>
            <separator name="group4"/>
            <separator name="group5"/>
            <separator name="group6"/>
            <separator name="end"/>
         </menu>
      </actionSet>
   </extension>
   <extension
         point="org.eclipse.ui.activities">
      <activityPatternBinding
            activityId="net.sourceforge.eclipsetrader"
            pattern="net.sourceforge.eclipsetrader.charts/net.sourceforge.eclipsetrader.views.chart"/>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="net.sourceforge.eclipsetrader.charts.Perspective"
            icon="icons/eview16/chart_curve.png"
            id="net.sourceforge.eclipsetrader.charts"
            name="%perspectiveName"/>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension targetID="net.sourceforge.eclipsetrader">
         <view
               id="net.sourceforge.eclipsetrader.views.chart:*"
               relationship="stack"
               relative="center"
               visible="false"/>
         <view
               id="net.sourceforge.eclipsetrader.chartData"
               relationship="stack"
               relative="bottom"
               visible="false"/>
         <perspectiveShortcut id="net.sourceforge.eclipsetrader.charts"/>
      </perspectiveExtension>
      <perspectiveExtension targetID="net.sourceforge.eclipsetrader.charts">
         <view
               id="net.sourceforge.eclipsetrader.views.securities"
               relationship="stack"
               relative="left"/>
         <view
               id="net.sourceforge.eclipsetrader.views.chart:*"
               relationship="stack"
               relative="center"
               visible="false"/>
         <view
               id="net.sourceforge.eclipsetrader.chartData"
               relationship="stack"
               relative="bottom"
               visible="false"/>
         <actionSet id="net.sourceforge.eclipsetrader.securities"/>
         <viewShortcut id="net.sourceforge.eclipsetrader.securities"/>
         <viewShortcut id="net.sourceforge.eclipsetrader.chartData"/>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <viewerContribution
            id="chartsView"
            targetID="net.sourceforge.eclipsetrader.views.chart">
         <action
               class="net.sourceforge.eclipsetrader.charts.actions.EditObjectAction"
               enablesFor="1"
               id="net.sourceforge.eclipsetrader.charts.editIndicator"
               label="%editObjectAction"
               menubarPath="top"
               style="push">
         </action>
         <action
               class="net.sourceforge.eclipsetrader.charts.actions.NewIndicatorAction"
               id="net.sourceforge.eclipsetrader.charts.newIndicator"
               label="%newIndicatorAction"
               menubarPath="top"
               style="push"/>
      </viewerContribution>
      <viewerContribution
            id="watchlistView"
            targetID="net.sourceforge.eclipsetrader.watchlist">
         <action
               class="net.sourceforge.eclipsetrader.charts.actions.OpenChartAction"
               enablesFor="1"
               id="net.sourceforge.eclipsetrader.actions.openChart"
               label="%openChartAction"
               menubarPath="top"
               style="pulldown"/>
      </viewerContribution>
      <viewerContribution
            id="securitiesView"
            targetID="net.sourceforge.eclipsetrader.views.securities">
         <action
               class="net.sourceforge.eclipsetrader.charts.actions.OpenChartAction"
               enablesFor="1"
               icon="icons/eview16/chart_curve.png"
               id="net.sourceforge.eclipsetrader.actions.openChart"
               label="%openChartAction"
               menubarPath="top"
               style="pulldown"/>
      </viewerContribution>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="net.sourceforge.eclipsetrader.charts.preferences.ChartsPrefereces"
            id="net.sourceforge.eclipsetrader.charts.general"
            name="Charts"/>
      <page
            category="net.sourceforge.eclipsetrader.charts.general"
            class="net.sourceforge.eclipsetrader.charts.preferences.DefaultConfigurationPage"
            id="net.sourceforge.eclipsetrader.charts.default"
            name="Default Settings"/>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup class="net.sourceforge.eclipsetrader.charts.internal.EarlyStartup"/>
   </extension>
   <extension
         point="org.eclipse.ui.themes">
      <themeElementCategory
            id="net.sourceforge.eclipsetrader.charts"
            label="%themeElementCategory.label.charts"/>
      <colorDefinition
            categoryId="net.sourceforge.eclipsetrader.charts"
            id="CHART_BACKGROUND"
            label="%colorDefinition.label.background"
            value="255,255,224"/>
      <colorDefinition
            categoryId="net.sourceforge.eclipsetrader.charts"
            id="CHART_SCALE_BACKGROUND"
            label="%colorDefinition.label.scale.background"
            value="255,255,255"/>
      <colorDefinition
            categoryId="net.sourceforge.eclipsetrader.charts"
            id="CHART_SCALE_FOREGROUND"
            label="%colorDefinition.label.scale.foreground"
            value="0,0,0"/>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="true"
            category="net.sourceforge.eclipsetrader"
            class="net.sourceforge.eclipsetrader.charts.views.ChartView"
            icon="icons/eview16/chart_curve.png"
            id="net.sourceforge.eclipsetrader.views.chart"
            name="%chartViewName"/>
      <view
            category="net.sourceforge.eclipsetrader"
            class="net.sourceforge.eclipsetrader.charts.views.DataView"
            icon="icons/eview16/database_table.png"
            id="net.sourceforge.eclipsetrader.chartData"
            name="%chartDataViewName"/>
   </extension>
   <extension
         point="org.eclipse.ui.viewActions">
      <viewContribution
            id="chartView"
            targetID="net.sourceforge.eclipsetrader.views.chart">
         <menu
               id="indicator"
               label="%menu.label.indicator"
               path="group1"/>
         <action
               class="net.sourceforge.eclipsetrader.charts.actions.DeleteObjectAction"
               enablesFor="1"
               id="net.sourceforge.eclipsetrader.charts.deleteIndicator"
               label="%deleteObjectAction"
               menubarPath="group1"
               style="push">
         </action>
         <action
               class="net.sourceforge.eclipsetrader.charts.actions.EditObjectAction"
               enablesFor="1"
               id="net.sourceforge.eclipsetrader.charts.editIndicator"
               label="%editObjectAction"
               menubarPath="group1"
               style="push">
         </action>
         <action
               class="net.sourceforge.eclipsetrader.charts.actions.NewIndicatorAction"
               icon="icons/elcl16/chart_line_add.png"
               id="net.sourceforge.eclipsetrader.charts.newIndicator"
               label="%newIndicatorAction"
               menubarPath="group1"
               style="push"/>
         <action
               class="net.sourceforge.eclipsetrader.charts.actions.UpdateChartAction"
               icon="icons/elcl16/refresh.gif"
               id="net.sourceforge.eclipsetrader.charts.refresh"
               label="%updateChartAction"
               menubarPath="group4"
               style="push"
               toolbarPath="refresh"
               tooltip="%updateChartTooltip"/>
         <action
               class="net.sourceforge.eclipsetrader.charts.objects.LineAction"
               icon="icons/etool16/line.gif"
               id="net.sourceforge.eclipsetrader.charts.line"
               label="%lineObjectAction"
               style="push"
               toolbarPath="objects"
               tooltip="%lineObjectTooltip"/>
         <action
               class="net.sourceforge.eclipsetrader.charts.objects.HorizontalLineAction"
               icon="icons/etool16/horizontalLine.gif"
               id="net.sourceforge.eclipsetrader.charts.horizontalLine"
               label="%horizontalLineAction"
               style="push"
               toolbarPath="objects"
               tooltip="%horizontalLineTooltip"/>
         <action
               class="net.sourceforge.eclipsetrader.charts.objects.FiboLineAction"
               icon="icons/etool16/fiboline.gif"
               id="net.sourceforge.eclipsetrader.charts.fiboline"
               label="%fibolineAction"
               style="push"
               toolbarPath="objects"
               tooltip="%fibolineTooltip"/>
         <action
               class="net.sourceforge.eclipsetrader.charts.actions.DeleteChartAction"
               id="net.sourceforge.eclipsetrader.charts.delete"
               label="Delete Chart"
               menubarPath="bottom"
               style="push"/>
      </viewContribution>
      <viewContribution
            id="securitiesView"
            targetID="net.sourceforge.eclipsetrader.views.securities">
         <action
               class="net.sourceforge.eclipsetrader.charts.actions.OpenChartAction"
               enablesFor="1"
               icon="icons/eview16/chart_curve.png"
               id="net.sourceforge.eclipsetrader.actions.openChart"
               label="%openChartAction"
               menubarPath="top"
               style="push"/>
         <action
               class="net.sourceforge.eclipsetrader.charts.actions.UpdateAllChartsAction"
               icon="icons/elcl16/refresh.gif"
               id="net.sourceforge.eclipsetrader.charts.updateAll"
               label="%updateAllChartsAction"
               menubarPath="bottom"
               style="push"/>
      </viewContribution>
      <viewContribution
            id="watchlistView"
            targetID="net.sourceforge.eclipsetrader.watchlist">
         <action
               class="net.sourceforge.eclipsetrader.charts.actions.OpenChartAction"
               enablesFor="1"
               id="net.sourceforge.eclipsetrader.actions.openChart"
               label="%openChartAction"
               menubarPath="top"
               style="push"/>
      </viewContribution>
   </extension>
