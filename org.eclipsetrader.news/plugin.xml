<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="org.eclipsetrader.news.internal.preferences.NewsPreferencesPage"
            id="org.eclipsetrader.news"
            name="News">
      </page>
      <page
            category="org.eclipsetrader.news"
            class="org.eclipsetrader.news.internal.preferences.RSSPreferencesPage"
            id="org.eclipsetrader.news.rss"
            name="RSS News">
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.eclipsetrader.news.internal.ui.NewsViewer"
            icon="icons/eview16/internal_browser.gif"
            id="org.eclipsetrader.news.viewer"
            name="News">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.elementFactories">
      <factory
            class="org.eclipsetrader.news.internal.ui.NewsViewerInputFactory"
            id="org.eclipsetrader.news.viewerInputFactory">
      </factory>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="org.eclipsetrader.news.internal.ui.Perspective"
            icon="icons/eview16/newspaper.png"
            id="org.eclipsetrader.news.perspective"
            name="News">
      </perspective>
   </extension>

</plugin>
