<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.themes">
      <themeElementCategory
            id="instrumentsExplorer"
            label="Instruments Explorer"/>
      <colorDefinition
            categoryId="instrumentsExplorer"
            id="EXPLORER_BACKGROUND"
            label="Background"
            value="224,224,255"/>
      <colorDefinition
            categoryId="instrumentsExplorer"
            id="EXPLORER_FOREGROUND"
            label="Foreground"
            value="0,0,0"/>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="net.sourceforge.eclipsetrader"
            class="net.sourceforge.eclipsetrader.internal.ui.views.explorer.SecurityExplorer"
            icon="icons/full/eview16/database.png"
            id="net.sourceforge.eclipsetrader.views.securities"
            name="%securitiesView"/>
   </extension>

</plugin>
