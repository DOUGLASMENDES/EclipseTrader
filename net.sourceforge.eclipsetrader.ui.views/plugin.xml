   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="net.sourceforge.eclipsetrader.TraderPerspective">
         <view
               visible="false"
               id="net.sourceforge.eclipsetrader.ui.views.NewsBrowser"
               relationship="stack"
               relative="org.eclipse.ui.editorss"/>
         <view
               visible="false"
               id="net.sourceforge.eclipsetrader.ui.views.ChartView:*"
               relationship="stack"
               relative="org.eclipse.ui.editorss"/>
         <view
               id="net.sourceforge.eclipsetrader.ui.views.Portfolio"
               relationship="stack"
               relative="org.eclipse.ui.editorss"/>
         <view
               id="net.sourceforge.eclipsetrader.ui.views.News"
               relationship="stack"
               relative="news"/>
         <view
               id="net.sourceforge.eclipsetrader.ui.views.indices"
               relative="strips"
               relationship="stack"/>
         <viewShortcut id="net.sourceforge.eclipsetrader.ui.views.Portfolio"/>
         <viewShortcut id="net.sourceforge.eclipsetrader.ui.views.News"/>
         <viewShortcut id="net.sourceforge.eclipsetrader.ui.views.indices"/>
         <actionSet id="net.sourceforge.eclipsetrader.ui.portfolio"/>
         <actionSet id="net.sourceforge.eclipsetrader.views.chartActionSet"/>
         <perspectiveShortcut id="net.sourceforge.eclipsetrader.ChartsPerspective"/>
         <perspectiveShortcut id="net.sourceforge.eclipsetrader.NewsPerspective"/>
      </perspectiveExtension>
      <perspectiveExtension targetID="net.sourceforge.eclipsetrader.ChartsPerspective">
         <view
               ratio="0.20"
               relative="org.eclipse.ui.editorss"
               relationship="left"
               id="net.sourceforge.eclipsetrader.ui.views.stockList"/>
         <view
               visible="false"
               id="net.sourceforge.eclipsetrader.ui.views.ChartView:*"
               relationship="stack"
               relative="org.eclipse.ui.editorss"/>
         <actionSet id="net.sourceforge.eclipsetrader.views.chartActionSet"/>
         <perspectiveShortcut id="net.sourceforge.eclipsetrader.NewsPerspective"/>
         <perspectiveShortcut id="net.sourceforge.eclipsetrader.TraderPerspective"/>
      </perspectiveExtension>
      <perspectiveExtension targetID="net.sourceforge.eclipsetrader.NewsPerspective">
         <view
               relationship="stack"
               relative="org.eclipse.ui.editorss"
               id="net.sourceforge.eclipsetrader.ui.views.NewsBrowser"/>
         <view
               ratio="0.75"
               relationship="bottom"
               relative="net.sourceforge.eclipsetrader.ui.views.NewsBrowser"
               id="net.sourceforge.eclipsetrader.ui.views.News"/>
         <viewShortcut id="net.sourceforge.eclipsetrader.ui.views.News"/>
         <viewShortcut id="net.sourceforge.eclipsetrader.ui.views.NewsBrowser"/>
         <viewShortcut id="net.sourceforge.eclipsetrader.ui.views.Portfolio"/>
         <perspectiveShortcut id="net.sourceforge.eclipsetrader.ChartsPerspective"/>
         <perspectiveShortcut id="net.sourceforge.eclipsetrader.TraderPerspective"/>
      </perspectiveExtension>
   </extension>
