            class="net.sourceforge.eclipsetrader.ui.views.news.NewsBrowser"
            icon="icons/news_tab.gif"
            category="trader"
            name="%news.browser"
            id="net.sourceforge.eclipsetrader.ui.views.NewsBrowser"/>
      <view
            allowMultiple="true"
